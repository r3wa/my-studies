    <artifactId>ws</artifactId>
    <packaging>war</packaging>
    <name>ws</name>

    <dependencies>
        <dependency>
            <groupId>javax</groupId>
            <artifactId>javaee-web-api</artifactId>
        </dependency>
    </dependencies>

    <build>
